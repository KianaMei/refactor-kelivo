{
  "name": "refactor-kelivo",
  "version": "0.1.0",
  "private": true,
  "description": "Kelivo（新一代）：Electron + React + TypeScript",
  "main": "./out/main/index.js",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "node scripts/run-electron-vite.mjs dev",
    "start": "node scripts/run-electron-vite.mjs preview",
    "build": "electron-vite build",
    "typecheck": "tsc -p tsconfig.node.json --noEmit && tsc -p tsconfig.web.json --noEmit",
    "pack": "yarn build && electron-builder --dir",
    "build:win": "yarn build && electron-builder --win --x64"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "electron": "^34.0.0",
    "lucide-react": "^0.563.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "jszip": "^3.10.1",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "electron-builder": "^26.0.0",
    "electron-vite": "^4.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
